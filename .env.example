# VNC Configuration
# Password for VNC access (default: android)
VNC_PASSWORD=android

# Emulator Configuration
# Amount of RAM to allocate to the emulator in MB (default: 4096)
EMULATOR_MEMORY=4096

# Number of CPU cores to allocate (default: 4)
EMULATOR_CORES=4

# Emulator name (default: android_emulator)
EMULATOR_NAME=android_emulator

# Display Configuration
# Virtual display number (default: 99)
DISPLAY=:99
